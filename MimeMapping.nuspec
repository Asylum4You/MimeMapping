			<dependency id="Microsoft.NETCore.Portable.Compatibility" version="1.0.1" />
			<dependency id="NETStandard.Library" version="1.6.0" />
		</dependencies>
	</metadata>
	<files>
		<file src="bin\Release\*.dll" target="lib\netstandard1.0" />
	</files>
</package>